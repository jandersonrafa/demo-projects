{
  "title": "KPIs - Python",
  "tags": [
    "stack",
    "python",
    "kpi"
  ],
  "schemaVersion": 39,
  "version": 2,
  "refresh": "15s",
  "tooltip": {
    "mode": "all"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "label": "Service",
        "type": "query",
        "datasource": {
          "type": "prometheus"
        },
        "query": {
          "query": "label_values(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=~\"python-.*\"}, container_label_com_docker_compose_service)",
          "refId": "A"
        },
        "refresh": 1,
        "multi": true,
        "includeAll": true,
        "allValue": "python-.*"
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "CPU usage (cores)",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (container_label_com_docker_compose_service) (rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=~\"$service\"}[1m]))",
          "legendFormat": "{{container_label_com_docker_compose_service}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Memory usage (MB)",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (container_label_com_docker_compose_service) (container_memory_usage_bytes{container_label_com_docker_compose_service=~\"$service\"}) / 1024 / 1024",
          "legendFormat": "{{container_label_com_docker_compose_service}} allocated"
        },
        {
          "expr": "sum by (container_label_com_docker_compose_service) (container_memory_working_set_bytes{container_label_com_docker_compose_service=~\"$service\"}) / 1024 / 1024",
          "legendFormat": "{{container_label_com_docker_compose_service}} used"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP requests (rps)",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by (job) (rate(http_requests_total{job=~\"$service\", uri!~\"/actuator.*|/metrics\"}[1m]))",
          "legendFormat": "{{job}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Process CPU Usage (cores)",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 3
        }
      },
      "targets": [
        {
          "expr": "sum by (job) (rate(process_cpu_seconds_total{job=~\"$service\"}[1m]))",
          "legendFormat": "{{job}} CPU Usage"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency (P95, P99) - ms",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, job) ((rate(http_request_duration_seconds_bucket{job=~\"$service\", uri!~\"/actuator.*|/metrics\"}[1m])))) * 1000",
          "legendFormat": "{{job}} P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, job) ((rate(http_request_duration_seconds_bucket{job=~\"$service\", uri!~\"/actuator.*|/metrics\"}[1m])))) * 1000",
          "legendFormat": "{{job}} P99"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Python Process Info",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      },
      "targets": [
        {
          "expr": "sum by (job) (python_info{job=~\"$service\"})",
          "legendFormat": "{{job}} version"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency Average per endpoint - ms",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      },
      "targets": [
        {
          "expr": "sum by (handler, job) (rate(http_request_duration_seconds_sum{job=~\"$service\", handler!~\"/actuator.*|/metrics\"}[1m])) / sum by (handler, job) (rate(http_request_duration_seconds_count{job=~\"$service\", handler!~\"/actuator.*|/metrics\"}[1m])) * 1000",
          "legendFormat": "{{job}} {{handler}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "App Errors (4xx|5xx)",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum by (job) (  increase(http_requests_total{job=~\"$service\", handler!~\"/actuator.*|/metrics\", path!~\"/actuator.*|/metrics\", status=~\"4..|5..\"}[1m]))",
          "legendFormat": "{{job}} errors"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Memory Fisica RSS (MB)",
      "datasource": {
        "type": "prometheus"
      },
      "options": {
        "tooltip": {
          "mode": "all"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "unit": "mbytes",
          "decimals": 1
        }
      },
      "targets": [
        {
          "expr": "sum by (job) (process_resident_memory_bytes{job=~\"$service\"}) / 1024 / 1024",
          "legendFormat": "{{job}} Mem Fisica"
        }
      ]
    }
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  }
}