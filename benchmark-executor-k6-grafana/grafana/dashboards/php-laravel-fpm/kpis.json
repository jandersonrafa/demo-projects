{
    "title": "KPIs - PHP Laravel FPM",
    "tags": [
        "stack",
        "php-fpm",
        "kpi"
    ],
    "schemaVersion": 39,
    "version": 1,
    "refresh": "15s",
    "tooltip": {
        "mode": "all"
    },
    "templating": {
        "list": [
            {
                "name": "service",
                "label": "Service",
                "type": "query",
                "datasource": {
                    "type": "prometheus"
                },
                "query": {
                    "query": "label_values(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=~\"php-fpm-.*\"}, container_label_com_docker_compose_service)",
                    "refId": "A"
                },
                "refresh": 1,
                "multi": true,
                "includeAll": true,
                "allValue": "php-fpm-.*"
            }
        ]
    },
    "panels": [
        {
            "type": "timeseries",
            "title": "CPU usage (cores)",
            "datasource": {
                "type": "prometheus"
            },
            "options": {
                "tooltip": {
                    "mode": "all"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "sum by (container_label_com_docker_compose_service) (rate(container_cpu_usage_seconds_total{container_label_com_docker_compose_service=~\"$service\"}[1m]))",
                    "legendFormat": "{{container_label_com_docker_compose_service}}"
                }
            ]
        },
        {
            "type": "timeseries",
            "title": "Memory usage (MB)",
            "datasource": {
                "type": "prometheus"
            },
            "options": {
                "tooltip": {
                    "mode": "all"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "sum by (container_label_com_docker_compose_service) (container_memory_usage_bytes{container_label_com_docker_compose_service=~\"$service\"}) / 1024 / 1024",
                    "legendFormat": "{{container_label_com_docker_compose_service}}"
                }
            ]
        },
        {
            "type": "timeseries",
            "title": "HTTP requests (rps)",
            "datasource": {
                "type": "prometheus"
            },
            "options": {
                "tooltip": {
                    "mode": "all"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "short",
                    "decimals": 0
                },
                "overrides": []
            },
            "targets": [
                {
                    "expr": "sum by (job) (rate(http_requests_total{job=~\"$service\", uri!~\"/actuator.*|/metrics\"}[1m]))",
                    "legendFormat": "{{job}} RPS"
                }
            ]
        },
        {
            "type": "timeseries",
            "title": "HTTP Latency (P95, P99) - ms",
            "datasource": {
                "type": "prometheus"
            },
            "options": {
                "tooltip": {
                    "mode": "all"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 16
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "ms",
                    "decimals": 1
                }
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum by (le, job) ((rate(http_request_duration_seconds_bucket{job=~\"$service\", uri!~\"/actuator.*|/metrics\"}[1m])))) * 1000",
                    "legendFormat": "{{job}} P95"
                },
                {
                    "expr": "histogram_quantile(0.99, sum by (le, job) ((rate(http_request_duration_seconds_bucket{job=~\"$service\", uri!~\"/actuator.*|/metrics\"}[1m])))) * 1000",
                    "legendFormat": "{{job}} P99"
                }
            ]
        },
        {
            "type": "timeseries",
            "title": "PHP Internal Peak Memory (MB)",
            "datasource": {
                "type": "prometheus"
            },
            "options": {
                "tooltip": {
                    "mode": "all"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 24
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "mbytes",
                    "decimals": 1
                }
            },
            "targets": [
                {
                    "expr": "sum by (job) (php_memory_peak_usage_bytes{job=~\"$service\"}) / 1024 / 1024",
                    "legendFormat": "{{job}}"
                }
            ]
        },
        {
            "type": "timeseries",
            "title": "HTTP Latency Average per endpoint - ms",
            "datasource": {
                "type": "prometheus"
            },
            "options": {
                "tooltip": {
                    "mode": "all"
                }
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "ms"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 32
            },
            "targets": [
                {
                    "expr": "sum by (job, path, status) (rate(http_request_duration_seconds_sum{job=~\"$service\", path!~\"/actuator.*|/metrics\"}[1m])) / sum by (job, path, status) (rate(http_request_duration_seconds_count{job=~\"$service\", path!~\"/actuator.*|/metrics\"}[1m])) * 1000",
                    "legendFormat": "{{job}} {{status}} {{path}}"
                }
            ]
        }
    ],
    "time": {
        "from": "now-30m",
        "to": "now"
    }
}